{
  "name": "binar-coba",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "tailwind:css": "tailwindcss -o ./public/css/style.css -w",
    "concat-css": "cat css/* > ./public/output/all.css",
    "dev": "concurrently --kill-others \"npm run tailwind:css\" \"npm run start\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "css-concat": "^0.0.4",
    "tailwindcss": "^2.2.8"
  }
}
